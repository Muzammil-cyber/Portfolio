 query getProjects($first:Int,$after:String)
 {
   projectsConnection(first:$first,after:$after) {
     edges {
       cursor
       node {
         id
         title
       }
     }
     pageInfo {
       hasNextPage
       hasPreviousPage
       startCursor
       endCursor
       pageSize
     }
   }
 }


